{
    "name": "datashaman/anvil",
    "description": "Artisan on the web",
    "keywords": [
        "datashaman",
        "artisan",
        "commands",
        "console"
    ],
    "homepage": "https://github.com/datashaman/anvil",
    "license": "MIT",
    "authors": [
        {
            "name": "Marlin Forbes",
            "email": "marlinf@datashaman.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "laravel/framework": "~5.8.0|^6.0|^7.0",
        "symfony/var-dumper": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^3.8|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Datashaman\\Anvil\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Datashaman\\Anvil\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Datashaman\\Anvil\\AnvilServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
